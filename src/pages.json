{
  "easycom": {
    "autoscan": true,
    "custom": {
      "nut-(.*)?-(.*)": "uni-nutui/components/sky-nutui/packages/__VUE/$1$2/index.vue",
      "nut-(.*)": "uni-nutui/components/sky-nutui/packages/__VUE/$1/index.vue"
    }
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/tabbar/tabbar",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/demo/index",
      "style": {
        "navigationBarTitleText": "demo"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "subPackages": [
    {
      "root": "pagesA",
      "pages": [
        {
          "path": "demo/index",
          "style": {
            "navigationBarTitleText": "pagesA"
          }
        }
      ]
    },
    {
      "root": "pagesB",
      "pages": [
        {
          "path": "demo/index",
          "style": {
            "navigationBarTitleText": "pagesB"
          }
        }
      ]
    }
  ],
  "preloadRule": {
    "pages/tabbar/tabbar": {
      "network": "all",
      "packages": ["pagesA"]
    }
  }
}
